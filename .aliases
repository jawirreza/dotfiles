alias dir2iso="python ~/Project/tools/dir2iso.py"
alias ytrename="python ~/Project/tools/Batch\ renamer.py"
alias neofetch="neofetch --ascii ~/.config/neofetch/chess.txt"
alias upgrade="pekmen;sudo reflector --latest 5 --protocol http --protocol https --sort rate --save /etc/pacman.d/mirrorlist;sudo pacman -Syyuw"
alias mirrorscreen="xrandr --output LVDS1 --mode 1366x768 --primary --output VGA1 --same-as LVDS1 --mode 1024x576"
alias extendsleft="xrandr --output LVDS1 --mode 1366x768 --primary --output VGA1 --same-as LVDS1 --mode 1024x576 --left-of LVDS1"
alias extendsright="xrandr --output LVDS1 --mode 1366x768 --primary --output VGA1 --same-as LVDS1 --mode 1024x576 --right-of LVDS1"
alias nomirrorscreen="xrandr --output LVDS1 --auto"
